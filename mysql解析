SELECT id,
       reimbursement_id,
       barcode,
       file_name,
       out_file_id,
       create_ts,
       create_user_id
FROM fill_reimbursement_bill
WHERE (reimbursement_id = '1JEONH8TA21MDC002A0A0000C02EFFF3')

SELECT id,
       reimbursement_no,
       reimbursement_data,
       create_ts,
       push_status_zy,
       create_user_id,
       fail_count,
       proc_status,
       apply_reason,
       process_id,
       push_audit_status,
       push_audit_count,
       open_url,
       push_audit_result_status,
       push_audit_result_count,
       audit_status,
       customer_id
FROM fill_reimbursement_data
WHERE (reimbursement_no = 202601120008)
ORDER BY create_ts DESC
